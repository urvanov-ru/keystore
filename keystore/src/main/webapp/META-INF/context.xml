<?xml version="1.0" encoding="UTF-8"?>
<Context>
    <!-- postgresql driver should be in lib folder of apache tomcat. -->
    <Resource name="jdbc/keyStoreDevDB" auth="Container"
        type="javax.sql.DataSource" username="postgres" password="postgres"
        driverClassName="org.postgresql.Driver" url="jdbc:postgresql://localhost:5432/keystore"
        maxActive="8" maxIdle="4" />
</Context>